  "engines": {
    "node": "15 || 16 || 17",
    "npm": ">=6"
  },
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack --mode production --watch",
    "build:dev": "webpack --mode development",
    "watch:dev": "webpack --mode development --watch",
    "test": "jest --json --outputFile=.jest-test-results.json || true",
    "test:watch": "jest --json --outputFile=.jest-test-results.json --watch || true",
    "start": "webpack-dev-server --open --mode development --watch",
    "start:live": "webpack-dev-server --open --mode development --liveReload",
    "launch": "rm -rf ./dist && concurrently -k \"yarn watch:dev\" \"yarn storybook\" \"yarn federate\"",
    "storybook": "start-storybook -s public -p 6006",
    "build-storybook": "build-storybook",
    "federate": "serve ./dist -p 3001",
    "setup": "node setup",
    "prepare": "husky install",
    "validate": "yarn test",
    "generate-component": "node ./src/util/cli/generateComponent/index.ts && yarn test ",
    "msw:update": "msw init public"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.6",
    "@babel/core": "7.9.6",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-jest": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storysource": "^6.1.11",
    "@storybook/addons": "^6.3.12",
    "@storybook/node-logger": "^6.1.11",
    "@storybook/react": "^6.3.8",
    "@storybook/theming": "^6.3.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.5.0",
    "concurrently": "^6.2.1",
    "css-loader": "^3.5.3",
    "graphql": "^15.4.0",
    "husky": "^7.0.4",
    "inquirer": "^8.1.4",
    "jest": "^27.2.0",
    "marked": "^3.0.4",
    "msw": "^0.35.0",
    "msw-storybook-addon": "^1.4.1",
    "serve": "^12.0.1",
    "storybook-addon-performance": "^0.16.1",
    "storybook-readme": "^5.0.9",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "typescript-plugin-styled-components": "^2.0.0",
    "urql": "2.0.5",
    "webpack": "^5.59.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.5.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
